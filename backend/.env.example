# Ambiente
NODE_ENV=development

# Porta do servidor (Railway define automaticamente em produção)
PORT=3001

# Database (Railway cria automaticamente DATABASE_URL em produção)
DATABASE_URL=postgresql://usuario:senha@localhost:5432/estoque_fotografia

# JWT Secret (GERAR UM TOKEN SEGURO EM PRODUÇÃO!)
# Use: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET=sua_chave_super_secreta_minimo_32_caracteres_aqui

# Frontend URL (para CORS)
FRONTEND_URL=http://localhost:3000

# API
API_VERSION=v1
